#%RAML 1.0
---
title: query
baseUri: http://nidi.guru/raml/{version}
version: v1
mediaType: application/default
schemas:
  String:
    schema: !include string.json

/data:
  get:
    responses:
      200:
        body:
          application/json:
            schema: |
              {"type":"string"}
/query:
  get:
    queryParameters:
      rep:
        type: string[]
      req:
      int:
        required: false
        type: integer
    responses:
      200:
        body:
          application/json:
            schema: |
              {"type":"string"}
